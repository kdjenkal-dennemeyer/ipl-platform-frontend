{
  "name": "ipl-platform-frontend",
  "version": "0.0.0",
  "private": true,
  "description": "IPL Platform Frontend Monorepo",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:shell": "turbo run dev --filter=@ipl-platform-frontend/shell",
    "dev:lounge": "turbo run dev --filter=@ipl-platform-frontend/lounge",
    "dev:portfolio": "turbo run dev --filter=@ipl-platform-frontend/portfolio",
    "dev:recordals": "turbo run dev --filter=@ipl-platform-frontend/recordals",
    "dev:epv": "turbo run dev --filter=@ipl-platform-frontend/epv",
    "build": "turbo run build",
    "type-check": "turbo run type-check",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "rm -rf node_modules .turbo && turbo run clean"
  },
  "devDependencies": {
    "babel-plugin-react-compiler": "^1.0.0",
    "turbo": "^2.3.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  }
}
